{
  "name": "clean-node-api",
  "version": "2.5.5",
  "main": "index.js",
  "author": "deepzS2",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist/* && tsc -p tsconfig.build.json",
    "postbuild": "copyfiles -u 1 public/**/* dist/static",
    "debug": "node --watch --inspect=0.0.0.0:9222 --nolazy dist/main/server.js",
    "dev": "sucrase-node src/main/server.ts",
    "up": "yarn build && docker-compose up -d",
    "test": "vitest run --silent --dangerouslyIgnoreUnhandledErrors",
    "test:verbose": "vitest",
    "test:unit": "vitest -c vitest-unit.config.ts",
    "test:integration": "vitest -c vitest-integration.config.ts",
    "test:staged": "yarn test",
    "test:ci": "yarn test --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@vitest/coverage-c8": "^0.25.2",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.27.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "mockdate": "^3.0.5",
    "rimraf": "^3.0.2",
    "sucrase": "^3.29.0",
    "supertest": "^6.3.1",
    "typescript": "4.9.3",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.25.2",
    "vitest-mongodb": "^0.0.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "fast-glob": "^3.2.12",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongo-round": "^1.0.0",
    "mongodb": "^4.12.0",
    "swagger-ui-express": "^4.6.0",
    "validator": "^13.7.0"
  },
  "engines": {
    "node": "18.x"
  },
  "_moduleAliases": {
    "@": "./dist"
  }
}
